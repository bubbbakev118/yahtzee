<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF"/>
            <corners android:radius="8dp"/>
            <stroke android:width="2dp" android:color="#CCCCCC"/>
        </shape>
    </item>
    <item android:gravity="top|start" android:left="12dp" android:top="12dp">
        <shape android:shape="oval">
            <solid android:color="#000000"/>
            <size android:width="8dp" android:height="8dp"/>
        </shape>
    </item>
    <item android:gravity="center|start" android:left="12dp">
        <shape android:shape="oval">
            <solid android:color="#000000"/>
            <size android:width="8dp" android:height="8dp"/>
        </shape>
    </item>
    <item android:gravity="bottom|start" android:left="12dp" android:bottom="12dp">
        <shape android:shape="oval">
            <solid android:color="#000000"/>
            <size android:width="8dp" android:height="8dp"/>
        </shape>
    </item>
    <item android:gravity="top|end" android:right="12dp" android:top="12dp">
        <shape android:shape="oval">
            <solid android:color="#000000"/>
            <size android:width="8dp" android:height="8dp"/>
        </shape>
    </item>
    <item android:gravity="center|end" android:right="12dp">
        <shape android:shape="oval">
            <solid android:color="#000000"/>
            <size android:width="8dp" android:height="8dp"/>
        </shape>
    </item>
    <item android:gravity="bottom|end" android:right="12dp" android:bottom="12dp">
        <shape android:shape="oval">
            <solid android:color="#000000"/>
            <size android:width="8dp" android:height="8dp"/>
        </shape>
    </item>
</layer-list>
